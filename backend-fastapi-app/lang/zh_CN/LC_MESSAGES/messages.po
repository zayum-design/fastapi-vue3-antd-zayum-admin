#: app/main.py:143 app/schemas/sys_user.py:52
msgid ""
"Username must start with a letter, can contain letters, numbers, and "
"underscores, and be 3-32 characters long."
msgstr "用户名必须以字母开头，可以包含字母、数字和下划线，长度为3-32个字符。"

#: app/api/api_v1/endpoints/user.py:44 app/api/api_v1/endpoints/user.py:56
#: app/api/api_v1/endpoints/user.py:66
msgid "User not found."
msgstr "未找到用户。"

#: app/crud/sys_user.py:23 app/crud/sys_user.py:52
#, python-format
msgid "Username '%(username)s' is already taken."
msgstr "用户名 '%(username)s' 已被占用。"

#: app/crud/sys_user.py:28 app/crud/sys_user.py:58
#, python-format
msgid "Email '%(email)s' is already registered."
msgstr "电子邮件 '%(email)s' 已被注册。"

#: app/crud/sys_user.py:33 app/crud/sys_user.py:64
#, python-format
msgid "Mobile number '%(mobile)s' is already in use."
msgstr "手机号码 '%(mobile)s' 已在使用中。"

#: app/schemas/sys_user.py:63
msgid ""
"Nickname can only contain letters, numbers, Chinese characters, spaces, "
"and hyphens, and must not exceed 50 characters."
msgstr "昵称只能包含字母、数字、中文字符、空格和连字符，且不得超过50个字符。"

#: app/schemas/sys_user.py:76
msgid "Password must be at least 6 characters long."
msgstr "密码长度必须至少为6个字符。"

#: app/schemas/sys_user.py:78
msgid "Password must contain at least one uppercase letter."
msgstr "密码必须包含至少一个大写字母。"

#: app/schemas/sys_user.py:80
msgid "Password must contain at least one lowercase letter."
msgstr "密码必须包含至少一个小写字母。"

#: app/schemas/sys_user.py:82
msgid "Password must contain at least one digit."
msgstr "密码必须包含至少一个数字。"

#: app/schemas/sys_user.py:93
msgid "Mobile number must contain only digits."
msgstr "手机号码只能包含数字。"

#: app/schemas/sys_user.py:95
msgid "Mobile number must be between 10 and 16 digits long."
msgstr "手机号码长度必须在10到16位数字之间。"

#: app/schemas/sys_user.py:108
msgid "A valid email address is required."
msgstr "需要一个有效的电子邮件地址。"

#: app/schemas/sys_user.py:118
msgid "Input should be either female or male"
msgstr "输入应为女性或男性。"

#: app/schemas/sys_user.py:128
msgid "Input should be either normal or hidden "
msgstr "输入应为正常或隐藏。"

#: app/utils/responses.py:24
msgid "Success"
msgstr "成功"
