# 端口号
VITE_PORT=5666

VITE_BASE=/

# web地址
VITE_GLOB_URL=http://api.www.zayum.com

# 接口地址
VITE_GLOB_API_URL=http://api.www.zayum.com/api

# 是否开启 Nitro Mock服务，true 为开启，false 为关闭
VITE_NITRO_MOCK=false

# 是否打开 devtools，true 为打开，false 为关闭
VITE_DEVTOOLS=false

# 是否注入全局loading
VITE_INJECT_APP_LOADING=true

# 登录过期时间（单位：小时）
VITE_LOGIN_EXPIRY_HOURS=2

# 应用命名空间
VITE_APP_NAMESPACE=zayum-admin

# 应用版本
VITE_APP_VERSION=1.0.2
